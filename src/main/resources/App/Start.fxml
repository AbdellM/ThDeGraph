<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="338.0" prefWidth="487.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.StartController">
    <children>
      <RadioButton fx:id="Orianted" focusTraversable="false" mnemonicParsing="false" prefHeight="27.0" prefWidth="147.0" text="Orianted" textOverrun="CENTER_WORD_ELLIPSIS">
         <font>
            <Font name="Calibri Bold" size="20.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="Weighted" focusTraversable="false" mnemonicParsing="false" prefHeight="27.0" prefWidth="145.0" text="Weighted">
         <font>
            <Font name="Calibri Bold" size="20.0" />
         </font>
      </RadioButton>
        <Label text="Select Your Graph And Stared Drawing It" />
        <Button fx:id="StartBtn" defaultButton="true" onAction="#switchToPrimary" prefHeight="26.0" prefWidth="186.0" text="GET STARTED" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
