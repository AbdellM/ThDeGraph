<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="100.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.RestartPopupController">
   <children>
      <Label layoutX="55.0" layoutY="23.0" prefHeight="18.0" prefWidth="490.0" text="This action will make lose all what you just draw, are you sure you want to restart drawing ?" AnchorPane.topAnchor="25.0" />
      <Button fx:id="closeBtn" defaultButton="true" focusTraversable="false" layoutX="437.0" layoutY="80.0" mnemonicParsing="false" onAction="#ClosePopup" text="Close" AnchorPane.bottomAnchor="20.0" />
      <Button fx:id="retartBtn" focusTraversable="false" layoutX="503.0" layoutY="80.0" mnemonicParsing="false" onAction="#Restart" text="Next" AnchorPane.bottomAnchor="20.0" />
   </children>
</AnchorPane>
